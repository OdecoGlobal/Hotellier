{
  "name": "server",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc --build",
    "start": "cross-env NODE_ENV=production nodemon ./dist/server.ts",
    "debug": "ndb ./src/server.ts",
    "seed": "ts-node src/db/seed.ts",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma --name",
    "prisma:studio": "prisma studio --schema=src/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/adapter-pg": "^6.8.2",
    "@prisma/client": "^6.8.2",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "ws": "^8.18.2",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.50",
    "@types/ws": "^8.18.1",
    "cross-env": "^7.0.3",
    "prisma": "^6.8.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
